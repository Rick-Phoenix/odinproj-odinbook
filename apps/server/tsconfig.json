{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node", "hono", "vitest"],
    "resolveJsonModule": true,
    "noEmit": true,
    "inlineSourceMap": true,
    "allowImportingTsExtensions": true
  },
  "include": [
    "src",
    "drizzle.config.ts",
    "scripts",
    "src/routes/chats/_chats-index.get.ts ",
    "vitest.config.ts",
    "./_static/.vite/parse-manifest.ts"
  ]
}
